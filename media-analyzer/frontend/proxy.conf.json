{
  "/api/**": {
    "target": "http://localhost:8000",
    "secure": false,
    "logLevel": "debug",
    "changeOrigin": true
  },
  "/streaming/**": {
    "target": "http://localhost:8081",
    "secure": false,
    "logLevel": "debug",
    "changeOrigin": true
  },
  "/ws/**": {
    "target": "http://localhost:8000",
    "secure": false,
    "ws": true,
    "changeOrigin": true
  }
}